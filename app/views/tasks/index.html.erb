<div class="col-md-12">
    <h1>My task</h1>

    <%= link_to "New", new_task_path, class: 'btn btn-primary' %>

    <% @tasks.each do |task| %>
        <%= render partial: 'task', object: task %>
    <% end %>

    <hr />

    <% @completed_task.each do |task| %>
        <%= render partial: 'task', object: task %>
    <% end %>
</div>